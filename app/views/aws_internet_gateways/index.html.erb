<h1>Listing aws_internet_gateways</h1>

<table>
  <thead>
    <tr>
      <th>Aws account</th>
      <th>Aws region</th>
      <th>Internet gateway</th>
      <th>Attachment vpc</th>
      <th>Attachment state</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @aws_internet_gateways.each do |aws_internet_gateway| %>
      <tr>
        <td><%= aws_internet_gateway.aws_account_id %></td>
        <td><%= aws_internet_gateway.aws_region_id %></td>
        <td><%= aws_internet_gateway.internet_gateway_id %></td>
        <td><%= aws_internet_gateway.attachment_vpc_id %></td>
        <td><%= aws_internet_gateway.attachment_state %></td>
        <td><%= link_to 'Show', aws_internet_gateway %></td>
        <td><%= link_to 'Edit', edit_aws_internet_gateway_path(aws_internet_gateway) %></td>
        <td><%= link_to 'Destroy', aws_internet_gateway, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Aws internet gateway', new_aws_internet_gateway_path %>
