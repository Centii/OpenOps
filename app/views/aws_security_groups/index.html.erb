<h1>Listing aws_security_groups</h1>

<table class="table table-condensed table-bordered table-hover table-striped">
  <thead>
    <tr>
      <th>Owner</th>
      <th>Group name</th>
      <th>Group</th>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @aws_security_groups.each do |aws_security_group| %>
      <tr>
        <td><%= aws_security_group.owner_id %></td>
        <td><%= aws_security_group.group_name %></td>
        <td><%= aws_security_group.group_id %></td>
        <td><%= aws_security_group.description %></td>
        <td><%= link_to 'Show', aws_security_group %></td>
        <td><%= link_to 'Edit', edit_aws_security_group_path(aws_security_group) %></td>
        <td><%= link_to 'Destroy', aws_security_group, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Aws security group', new_aws_security_group_path %>
