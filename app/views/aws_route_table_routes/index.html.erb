<h1>Listing aws_route_table_routes</h1>

<table>
  <thead>
    <tr>
      <th>Route table</th>
      <th>Destination cidr block</th>
      <th>Gateway</th>
      <th>Instance</th>
      <th>Instance owner</th>
      <th>Network interface</th>
      <th>Vpc peering connection</th>
      <th>State</th>
      <th>Origin</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @aws_route_table_routes.each do |aws_route_table_route| %>
      <tr>
        <td><%= aws_route_table_route.route_table_id %></td>
        <td><%= aws_route_table_route.destination_cidr_block %></td>
        <td><%= aws_route_table_route.gateway_id %></td>
        <td><%= aws_route_table_route.instance_id %></td>
        <td><%= aws_route_table_route.instance_owner_id %></td>
        <td><%= aws_route_table_route.network_interface_id %></td>
        <td><%= aws_route_table_route.vpc_peering_connection_id %></td>
        <td><%= aws_route_table_route.state %></td>
        <td><%= aws_route_table_route.origin %></td>
        <td><%= link_to 'Show', aws_route_table_route %></td>
        <td><%= link_to 'Edit', edit_aws_route_table_route_path(aws_route_table_route) %></td>
        <td><%= link_to 'Destroy', aws_route_table_route, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Aws route table route', new_aws_route_table_route_path %>
