<h1>Listing aws_security_group_pairs</h1>

<table>
  <thead>
    <tr>
      <th>Security group</th>
      <th>Permission set</th>
      <th>User</th>
      <th>Group name</th>
      <th>Group</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @aws_security_group_pairs.each do |aws_security_group_pair| %>
      <tr>
        <td><%= aws_security_group_pair.security_group_id %></td>
        <td><%= aws_security_group_pair.permission_set_id %></td>
        <td><%= aws_security_group_pair.user_id %></td>
        <td><%= aws_security_group_pair.group_name %></td>
        <td><%= aws_security_group_pair.group_id %></td>
        <td><%= link_to 'Show', aws_security_group_pair %></td>
        <td><%= link_to 'Edit', edit_aws_security_group_pair_path(aws_security_group_pair) %></td>
        <td><%= link_to 'Destroy', aws_security_group_pair, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Aws security group pair', new_aws_security_group_pair_path %>
