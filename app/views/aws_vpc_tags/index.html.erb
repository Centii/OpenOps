<h1>Listing aws_vpc_tags</h1>

<table>
  <thead>
    <tr>
      <th>Aws vpc</th>
      <th>Name</th>
      <th>Value</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @aws_vpc_tags.each do |aws_vpc_tag| %>
      <tr>
        <td><%= aws_vpc_tag.aws_vpc_id %></td>
        <td><%= aws_vpc_tag.name %></td>
        <td><%= aws_vpc_tag.value %></td>
        <td><%= link_to 'Show', aws_vpc_tag %></td>
        <td><%= link_to 'Edit', edit_aws_vpc_tag_path(aws_vpc_tag) %></td>
        <td><%= link_to 'Destroy', aws_vpc_tag, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Aws vpc tag', new_aws_vpc_tag_path %>
