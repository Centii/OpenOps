<h1>Listing ebs_volume_tags</h1>

<table>
  <thead>
    <tr>
      <th>Volume</th>
      <th>Key</th>
      <th>Value</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @ebs_volume_tags.each do |ebs_volume_tag| %>
      <tr>
        <td><%= ebs_volume_tag.volume_id %></td>
        <td><%= ebs_volume_tag.key %></td>
        <td><%= ebs_volume_tag.value %></td>
        <td><%= link_to 'Show', ebs_volume_tag %></td>
        <td><%= link_to 'Edit', edit_ebs_volume_tag_path(ebs_volume_tag) %></td>
        <td><%= link_to 'Destroy', ebs_volume_tag, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Ebs volume tag', new_ebs_volume_tag_path %>
